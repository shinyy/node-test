<!DOCTYPE html>
<html>
<head>
<title>helloworld</title>
<link rel='stylesheet' href='/stylesheets/style.css' />

<script type="text/javascript" src="js/jquery.min.js"></script>
<script type="text/javascript">
	$.getJSON("js/j.json",function(data){
		 var txt, x, xx, i, ii;
                txt = "<tr>";
                x = data.tr;
                console.log(x.length);
                //每页最多显示5条
                for (i = 0; i < x.length; i++) {
                    if (x[i]) {

                        xx = x[i].orderNum; {
                            try {
                                txt += "<td>" + xx + "</td>"
                            } catch(er) {
                                txt += "<td>&nbsp;</td>"
                            }
                        }

                        xx = x[i].phone;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].user;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].hotel;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].roomtype;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].roomNum;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].intime;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].leavetime;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].ordertime;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].state;
                        txt += "<td>" + xx + "</td>";
                        xx = x[i].tmorder;
                        txt += "<td>" + xx + "</td>";

                    }

                    txt += "</tr>";
                }
                console.log(txt)
                $(".sTable").html(txt);
	})
</script>
</head>
<body>
<h1>helloworld</h1>
<p>Welcome to helloworld</p>
<table class="sTable" border=1 style="text-align:center;">
</table>
</body>
</html>