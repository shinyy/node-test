                                             xx=x[i].hotel;
                                            txt+="<td>"+xx+"</td>";
                                             xx=x[i].roomtype;
                                            txt+="<td>"+xx+"</td>";
                                             xx=x[i].roomNum;
                                            txt+="<td>"+xx+"</td>";
                                             xx=x[i].intime;
                                            txt+="<td>"+xx+"</td>";
                                             xx=x[i].leavetime;
                                            txt+="<td>"+xx+"</td>";
                                             xx=x[i].ordertime;
                                            txt+="<td>"+xx+"</td>";
                                             xx=x[i].state;
                                            txt+="<td>"+xx+"</td>";
                                             xx=x[i].tmorder;
                                             txt+="<td>"+xx+"</td>";
                                             txt+="</tr>";
                                        }
                                        $(".sTable tbody").html(txt);

                                        //分页
                                        console.log(Math.ceil(x.length/5))

                                        for(var j=0;j<Math.ceil(x.length/5);j++){
                                            $(".previous.fg-button.ui-button").next("span").append($('<span class="fg-button ui-button">'+(j+1)+'</span>'))
                                        };
